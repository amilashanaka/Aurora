<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorAshBack"
    tools:context=".WelcomeActivity">

    <Button
        android:id="@+id/logOut"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="10dp"
        android:layout_marginEnd="329dp"
        android:layout_marginBottom="40dp"
        android:background="@drawable/logout"

        />

    <Button
        android:id="@+id/exit"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="40dp"
        android:background="@drawable/exit" />

    <TextView
        android:id="@+id/appTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignParentTop="true"
        android:layout_marginStart="67dp"
        android:layout_marginTop="34dp"
        android:text="WelCome To Aurora Store"
        android:textColor="@color/colorDarkAsh"
        android:textSize="20dp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="42dp" />


    <Button

        android:id="@+id/btn_deliveryNote"
        android:layout_width="298dp"
        android:layout_height="38dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="45dp"
        android:layout_marginBottom="311dp"
        android:background="@color/colorHoneydew"
        android:text="@string/btn1_welcome"
        android:textAlignment="center"
        android:textColor="@color/colorDarkAsh"
        android:textSize="14sp"


        tools:text="View Delivery Notes" />

    <Button
        android:id="@+id/btn_pack"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="45dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="164dp"
        android:background="@drawable/pack"
        android:textColor="@color/colorDarkAsh"

        android:visibility="invisible" />

    <Button
        android:id="@+id/userList2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="46dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="237dp"
        android:background="@drawable/prof"

        android:visibility="invisible" />

    <TextView
        android:id="@+id/viewStaff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="141dp"
        android:layout_marginBottom="177dp"
        android:text="Pack Section"
        android:textColor="@color/colorDarkAsh"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/viewStaff2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="196dp"
        android:layout_marginBottom="244dp"
        android:text="View Staff"
        android:textColor="@color/colorDarkAsh"
        android:visibility="invisible" />


    <TextView
        android:id="@+id/userType"
        android:layout_width="298dp"
        android:layout_height="38dp"

        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="45dp"
        android:layout_marginTop="155dp"
        android:background="#FAF2F2"
        android:text="User Type"
        android:textAlignment="center"
        android:textColor="@color/colorDarkAsh"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/userName"
        android:layout_width="298dp"
        android:layout_height="38dp"


        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="46dp"
        android:layout_marginTop="102dp"
        android:background="#FAF2F2"
        android:text="User Name"
        android:textAlignment="center"
        android:textColor="@color/colorDarkAsh"
        android:textSize="18sp" />

</RelativeLayout>